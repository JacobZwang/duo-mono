{
	"name": "duo-mono",
	"description": "Automatically log into Duo 2fa using this chrome extension. If you have chrome sync turned on it will work anywhere you are signed into chrome.",
	"scripts": {
		"build": "node esbuild.mjs",
		"watch:build": "node esbuild.mjs --watchbuild",
		"start": "node ./build/main.js",
		"watch:start": "node esbuild.mjs --watchbuild --watchstart",
		"build:start": "node esbuild.mjs && node ./build/main.js"
	},
	"author": "Jacob Zwang <zwang.jacob@gmail.com>",
	"devDependencies": {
		"@types/node": "^16.11.21",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"chalk": "^4.1.2",
		"esbuild": "^0.12.29",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"minimist": "^1.2.5",
		"prettier": "^2.5.1",
		"typescript": "^4.5.5"
	}
}